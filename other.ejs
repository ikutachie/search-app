    </head>
        <body class="container">
            <header>
                <h1 class="display-4"><%=title%></h1>
                <%var count =Object.keys(storeDic).length%>
                <span class="count">検索結果<%=count %>件<span>
            </header>
            <div role="main">
                <p><%=content%></p>
                <form method="POST" action="/other">
                <input type="text" name="msg" class="form-control">
                <input type="submit" value="Search" class="btn btn primary">
                </form>
                <table border="1" bordercolor="#f6efdb"cellspacing="0" cellpadding="0" class="table" align="center">
                  <tr><th>店名</th><th>住所</th><th>電話番号</th></tr>
                  <div class ="list-group">
                   <% for(var storeName in storeDic) {%>
                      <tr>
                          <div class="list-group-item">
                          <dl>
                            <%if(storeDic[storeName].detail.YUrl){%>
                              <td><a href=<%=storeDic[storeName].detail.YUrl%>><%= storeName%></a>　</td>
                              <%}else{%>
                                <td><%= storeName%></td>
                              <%}%>
                            <td><%=storeDic[storeName].address%>　</td>
                            <td><%=storeDic[storeName].tel%>　</td>
                          </dl>
                          </div>
                      </tr>
                    <%}%>
                  </div>
                </table>
            </div>
